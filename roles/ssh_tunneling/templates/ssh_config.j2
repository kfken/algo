Host algo
  #DynamicForward 127.0.0.1:1080
  LocalForward 9050 127.0.0.1:9050
  LogLevel quiet
  Compression yes
  IdentityFile {{ item }}.pem
  User {{ item }}
  Hostname {{ IP_subject_alt_name }}
  Port {{ ansible_ssh_port }}

Host algo6
  #DynamicForward 127.0.0.1:1080
  LocalForward 9050 127.0.0.1:9050
  LogLevel quiet
  Compression yes
  IdentityFile {{ item }}.pem
  User {{ item }}
  Hostname {{ ansible_default_ipv6['address'] }}
  Port {{ ansible_ssh_port }}
  